while True:
    print("#################")
    print(" made by bad_boy ")
    print("#################")
    print("[1]Multiplication")
    print("[2]Division")
    print("[3]Total")
    print("[4]Submission")
    print("[5]exit")
    king=int(input("=====>"))
    
if king == 1:
    joker=int(input("Write a number to multiply by the second number: "))
    opp=int(input("Enter the second number to be multiplied by the first number: "))
    print(joker*opp)
elif king == 2:
    fuck=int(input("Write a number to divide by the second number: "))
    olll=int(input("Write the second number: "))
    print(fuck//olll)
elif king == 3:
    kiki=int(input("Write a number to add to the second number: "))
    ttt=int(input("Write the second number to be added to the first number: "))
    print(kiki+ttt)
elif king == 4:
    hacker=int(input("Write for a number fraction greater than the second number: "))
    habol=int(input("Enter your number to subtract from the first number: "))
    print(hacker-habol)
else:
    print("exit...")
